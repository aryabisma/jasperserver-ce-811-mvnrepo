            <plugin>
                <groupId>com.github.wvengen</groupId>
                <artifactId>proguard-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>proguard</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <proguardVersion>${proguard.version}</proguardVersion>
                    <obfuscate>true</obfuscate>
                    <skip>${proguard.skip}</skip>
                    <injar>${project.build.finalName}.jar</injar>
                    <outjar>${project.build.finalName}.jar</outjar>
                    <outputDirectory>${project.build.directory}</outputDirectory>
                    <proguardInclude>${basedir}/src/main/resources/proguard.conf</proguardInclude>
                    <!-- Current version of the proguard doesn't support java 9 very well (warns about `META-INF/versions/9`) -->
                    <!-- Because plugin doesn't support filters adding manually lib dependencies with filter -->
                    <options>
                        <!-- because of the warning these libs where defined manually using `-libraryjars` option -->
                        <option>-libraryjars
                            ${settings.localRepository}/org/bouncycastle/bcprov-jdk15on/${bcprov.version}/bcprov-jdk15on-${bcprov.version}.jar(!META-INF/versions/**)
                        </option>
                        <option>-libraryjars
