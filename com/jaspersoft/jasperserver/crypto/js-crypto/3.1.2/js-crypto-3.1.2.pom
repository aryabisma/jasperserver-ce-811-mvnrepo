                        </option>
                        <option>-libraryjars
                            ${settings.localRepository}/org/apache/logging/log4j/log4j-1.2-api/${log4j.version}/log4j-1.2-api-${log4j.version}.jar(!META-INF/versions/9/**)
                        </option>
                        <option>-libraryjars
                            ${settings.localRepository}/org/apache/logging/log4j/log4j-core/${log4j.version}/log4j-core-${log4j.version}.jar(!META-INF/versions/9/**)
                        </option>
                    </options>
                    <exclusions>
                        <exclusion>
                            <!-- Exclude here because added manually in the options (-libraryjars) to enable filter -->
                            <groupId>org.bouncycastle</groupId>
                            <artifactId>bcprov-jdk15on</artifactId>
                        </exclusion>
                        <exclusion>
                            <!-- Exclude here because added manually in the options (-libraryjars) to enable filter -->
                            <groupId>org.bouncycastle</groupId>
                            <artifactId>bcpkix-jdk15on</artifactId>
                        </exclusion>
                        <exclusion>
                            <!-- Exclude here because added manually in the options (-libraryjars) to enable filter -->
                            <groupId>org.apache.logging.log4j</groupId>
                            <artifactId>log4j-api</artifactId>
                        </exclusion>
                        <exclusion>
                            <!-- Exclude here because added manually in the options (-libraryjars) to enable filter -->
                            <groupId>org.apache.logging.log4j</groupId>
                            <artifactId>log4j-1.2-api</artifactId>
                        </exclusion>
                        <exclusion>
                            <!-- Exclude here because added manually in the options (-libraryjars) to enable filter -->
                            <groupId>org.apache.logging.log4j</groupId>
                            <artifactId>log4j-core</artifactId>
                        </exclusion>
                        <exclusion>
                            <groupId>org.checkerframework</groupId>
                            <artifactId>checker</artifactId>
                        </exclusion>
